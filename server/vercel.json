{
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "crons": [
        {
            "path": "/api/test-send-stock-alerts",
            "schedule": "0 8 * * *",
            "timezone": "Europe/Madrid"
        },
        {
            "path": "/api/test-send-visits-email",
            "schedule": "0 15 * * 0",
            "timezone": "Europe/Madrid"
        }
    ]
}